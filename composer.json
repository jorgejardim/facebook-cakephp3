{
    "name": "jorge/cakephp-facebook",
    "description": "CakePHP 3 Facebook Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "facebook"],
    "license": "MIT",
    "authors": [
    {
        "name": "Jorge Jardim",
        "email": "jorge@jorgejardim.com.br"
    }
    ],
    "require": {
    	"php": ">=5.4",
    	"cakephp/cakephp": "~3.0",
    	"facebook/php-sdk-v4": "~5.0"
    },
    "require-dev": {
	   "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "JorgeFacebook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JorgeFacebook\\Test\\": "tests"
        }
    }
}